{
  "name": "Kegbot",
  "version": "0.0.2",
  "slug": "kegbot",
  "description": "Kegbot Server, the internet beer kegerator monitoring system.",
  "url": "https://github.com/johnpdowling/hassio-addons/kegbot",
  "ingress": false,
  "panel_icon": "mdi:keg",
  "startup": "application",
  "homeassistant": "0.92.0b2",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "ports": {
    "8000/tcp": 2080
  },
  "ports_description": {
    "8000/tcp": "Web interface"
  },
  "boot": "auto",
  "hassio_api": false,
  "hassio_role": "manager",
  "homeassistant_api": false,
  "host_network": false,
  "auth_api": false,
  "auto_uart": false,
  "gpio": false,
  "privileged": [
  ],
  "devices": [
  ],
  "apparmor": true,
  "map": [
    "config:rw",
    "ssl"
  ],
  "options": {
    "debug": true,
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "db_name": "kegbot",
    "db_host": "core-mariadb",
    "db_port": "3306",
    "db_user": "kegbot",
    "db_pass": ""
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)?",
    "debug": "bool",
    "ssl": "bool",
    "certfile": "str",
    "db_name": "str",
    "db_host": "str",
    "db_port": "int",
    "db_user": "str",
    "db_pass": "str",
    "email_from": "str?",
    "email_host": "str?",
    "email_port": "int?",
    "email_user": "str?",
    "email_pass": "str?",
    "email_ssl": "bool?",
    "email_tls": "bool?",
    "i_like_to_be_pwned": "bool?",
    "leave_front_door_open": "bool?"
  }
}
